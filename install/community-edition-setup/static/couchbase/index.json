{
  "gluu_user": {
    "attributes": [
      ["objectClass"], 
      ["creationDate"], 
      ["inum"], 
      ["del","exp"] 
    ],
    "static" : [
      [["LOWER(`uid`)"], "objectClass = \"gluuPerson\""], 
      [["LOWER(`mail`)"], "objectClass = \"gluuPerson\""], 
      [["oxExternalUid"], "objectClass = \"gluuPerson\""], 
      [["exp", "oxAuthExpiration"], "del=true"] 
    ]
  },

  "gluu_authorization": {
     "attributes": [
        ["oxAuthExpiration"],
        ["del"],
        ["oxAuthClientId"],
        ["oxAuthUserId"]
      ],
    "static" : [
    ]
  },

  "gluu": {
    "attributes": [
      ["objectClass"],
      ["oxScriptType"], 
      ["oxAuthExpiration"], 
      ["iat"], 
      ["oxAuthClientSecretExpiresAt"], 
      ["creationDate"], 
      ["tknCde"], 
      ["inum"], 
      ["displayName"], 
      ["oxAuthRegistrationAccessToken"], 
      ["oxAuthClaimName"], 
      ["mail"], 
      ["oxId"], 
      ["oxTicket"], 
      ["del"],
      ["ssnId"],
      ["oxAuthClientId"],
      ["clnId"],
      ["del","exp"],
      ["objectClass","creationDate","oxStatus"]
    ],
    "static" : [
      [["creationDate"], "objectClass = \"gluuPasswordResetRequest\""],
      [["oxScriptType"], "objectClass = \"oxCustomScript\""],
      [["oxApplicationType", "oxMetricType", "oxStartDate", "oxEndDate"], "objectClass = \"oxMetric\""],
      [["exp", "oxAuthExpiration"], "del=true"]
        
    ]
  },

  "gluu_site": {
    "attributes": [
        ["objectClass"], 
        ["inum"]
    ], 
    "static" : [
    ]
  },

  "gluu_statistic": {
    "attributes": [
      ["objectClass"], 
      ["oxStartDate"], 
      ["del"], 
      ["oxAuthExpiration"]
    ],
    "static" : [
    ]
  },

  "gluu_cache": {
    "attributes":[
      ["del","exp"]
    ],
    "static" : [
      [["exp", "oxAuthExpiration"], "del=true"],
      [["oxAuthExpiration"], "objectClass = \"cache\""]
    ]
  },

  "gluu_token": {
    "attributes": [
      ["del","exp"],
      ["authzCode"]
    ],
    "static" : [
      [["exp", "oxAuthExpiration"], "del=true"],
      [["grtId"], "objectClass = \"token\""],
      [["ssnId"], "objectClass = \"token\""]
    ]
  },

  "__common__": {
    "attributes":[
    ],
    "static":[
    ]
  }

}
